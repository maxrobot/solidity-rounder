{
    "name": "rounder",
    "version": "0.0.1",
    "description": "A smart contract application that can round inputs of integers to the nearest N",
    "author": "Friedrich Grabner",
    "scripts": {
        "clean": "rm -rf build node_modules",
        "compile": "truffle compile",
        "coverage": "solidity-coverage",
        "deploy": "truffle deploy --network development",
        "lint": "solhint contracts/**/*.sol",
        "test": "truffle test",
        "testrpc": "ganache-cli --port 8545 --gasLimit 0xFFFFFFFFFFF --gasPrice 0 --defaultBalanceEther 99999999999 --networkId 1234"
    },
    "license": "LGPL-3.0+",
    "devDependencies": {
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "ganache-cli": "6.1.0",
        "truffle": "5.8.4"
    }
}
